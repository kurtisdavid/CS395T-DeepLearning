#!/bin/bash
# AlexNet + SGD Experiments
python3 main.py --optim SGD --lr .1 --epochs 200 --model_file ./trained_models/alexnet_sgd.pt --log_file ./results/alexnet_sgd.pck --save_model_init ./init_models/alexnet_sgd.state
python3 main.py --optim SGD --lr .1 --epochs 200 -tv --model_file ./trained_models/alexnet_sgd_tv.pt --log_file ./results/alexnet_sgd_tv.pck --load_model_init ./init_models/alexnet_sgd.state
# AlexNet + Adam Experiments
python3 main.py --optim adam --lr .0005 --epochs 200 --model_file ./trained_models/alexnet_adam.pt --log_file ./results/alexnet_adam.pck --save_model_init ./init_models/alexnet_adam.state
python3 main.py --optim adam --lr .0005 --epochs 200 -tv --model_file ./trained_models/alexnet_adam_tv.pt --log_file ./results/alexnet_adam_tv.pck --load_model_init ./init_models/alexnet_adam.state
